<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dh.wms.mapper.StoreMapper">


    <select id="queryById" resultType="com.dh.wms.entity.Store">
        select id, name, location, create_time, update_time
        from store
        where id = #{id}
    </select>

</mapper>